  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "repository": "git@github.com:morgs32/storybook-addon-xd-designs.git",
  "homepage": "https://github.com/morgs32/storybook-addon-xd-designs",
